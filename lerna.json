{
  "packages": [
    "packages/*"
  ],
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md",
    "**/create-mor/**",
    "**/takin/**"
  ],
  "npmClient": "pnpm",
  "command": {
    "version": {
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): publish packages",
      "private": false
    },
    "publish": {
      "allowBranch": [
        "main",
        "beta",
        "feat/npm-publish"
      ],
      "message": "chore(release): publish packages"
    }
  },
  "version": "1.0.9-beta.1"
}
